{
  "video-text-editor": {
    "name": "Video Text Editor",
    "description": "Video editor con AI basado en transcripción",
    "category": "low-gpu",
    "path": "/media/franco/datos-bkp/SELF-HOSTED/video-text-editor",
    "defaultPort": 5173,
    "serviceScript": "./video-text-editor-service.sh",
    "healthCheck": {
      "type": "http",
      "path": "/",
      "timeout": 5000
    },
    "requirements": {
      "node": true,
      "npm": true,
      "ports": [5173]
    },
    "resources": {
      "ram": "low",
      "cpu": "low",
      "gpu": "none",
      "storage": "low"
    },
    "features": ["transcription", "video-editing", "browser-based"]
  },
  "penpot": {
    "name": "Penpot",
